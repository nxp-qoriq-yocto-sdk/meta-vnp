+				REPORT_ERROR(MAJOR, E_NO_MEMORY,
+							 ("en_cumulative_entry"));
+				retval = -1;
+				return retval;
+				
+			}
+			cumulative_entry = &cumulative_tbl_entry->cumulative_entry;
+			cumulative_entry->flags = EN_CUMULATIVE_NODE;
