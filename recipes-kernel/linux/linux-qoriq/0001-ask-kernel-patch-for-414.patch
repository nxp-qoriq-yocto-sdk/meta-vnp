+		
+	//determine number of bits set in the hash mask
+	ii = p_Param->hashResMask;
+#ifdef CONFIG_FMAN_ARM
+        __asm__ ("clz %0,%1\n"
+                        : "=r"(num_of_zeroes)
+                        : "r"(ii));
+#else
+        __asm__ ("cntlzw %0,%1\n"
+                        : "=r"(num_of_zeroes)
+                        : "r"(ii));
+#endif
