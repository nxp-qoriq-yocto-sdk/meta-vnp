+static void display_buf_data(void *dataptr, uint32_t size)
+{
+	uint32_t ii;
+	uint8_t *ptr;
+
+	ptr = (uint8_t *)dataptr;	
+	for (ii = 0; ii < size; ii++) {
+		if ((ii % 16) == 15) 
+			printk("%02x\n", *ptr);
+		else
+			printk("%02x ", *ptr);
+		ptr++;
+	}
+        if (ii % 16)
+		printk("\n");
+}
+
+static inline uint32_t swap_word(uint32_t *ptr)
+{
+	uint32_t val;
+	val =  ((*ptr >> 24) | (*ptr << 24) | 
+			((*ptr >> 8) & 0x0000ff00) |
+			((*ptr << 8) & 0xff0000));
+	return val;
+}
