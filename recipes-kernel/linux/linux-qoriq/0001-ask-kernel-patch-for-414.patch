+                case IPV6_UDP_TABLE:
+                case IPV6_TCP_TABLE:
+                case ESP_IPV6_TABLE:
+		case IPV4_3TUPLE_UDP_TABLE:
+		case IPV4_3TUPLE_TCP_TABLE:
+ 	       	case IPV6_3TUPLE_UDP_TABLE:
