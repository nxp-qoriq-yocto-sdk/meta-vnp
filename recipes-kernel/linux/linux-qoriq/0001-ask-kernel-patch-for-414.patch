+                info->pSpinlock = XX_MallocSmart((sizeof(t_Handle *) * (info->hashmask + 1)),
+                        0, sizeof(t_Handle));
+                if (!info->pSpinlock) {
+                        REPORT_ERROR(MAJOR, E_NO_MEMORY,
+                             ("spin lock array"));
+                        goto err_ret;
+                }
