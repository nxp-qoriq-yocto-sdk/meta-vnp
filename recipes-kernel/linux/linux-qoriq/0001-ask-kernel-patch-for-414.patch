+		for (ii = 0; ii < sizeof(struct en_ehash_entry); ii++) {
+			if ((ii % 16) == 0)
+				printk("\n");
+			printk("%02x ", *ptr);
+			ptr++;
+		}
+		printk("\n");
+	}
+#endif
+func_ret:
+    	XX_UnlockIntrSpinlock(h_Spinlock, intFlags);
+	return retval;	
+}
