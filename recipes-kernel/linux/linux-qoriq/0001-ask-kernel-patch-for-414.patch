+		printk("param ptr\t%p\n", param_ptr);
+		switch (*opc_ptr) {
+			case 0:
+				printk("end of opcodelist\n");
+				return;
+			case ENQUEUE_PKT:
+				param_ptr = display_enqparams_opc(param_ptr);	
+				return;
+			case REPLICATE_PKT:
+				param_ptr = display_replicate_opc(param_ptr);
