+int ExternalHashSetReasslyPool(uint32_t type, uint32_t ctx_bpid, 
+		uint32_t ctx_bsize, uint32_t frag_bpid, uint32_t frag_bsize, 
+		uint32_t txc_fqid, uint32_t ipr_timer_freq)
