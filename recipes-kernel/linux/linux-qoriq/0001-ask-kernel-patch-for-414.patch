+static void display_table_desc(uint32_t *ptr)
+{
+	uint32_t opcode;
+	
+	opcode = swap_word(ptr);
+	if ((opcode & 0xc0000000) == 0x40000000) {
+		opcode = swap_word(ptr + 2);
+		switch (opcode & 0xff) {
+			case 0x35:
+				display_hmtd(ptr);
+				break;
+			case 0x36:
+				display_stats_ad(ptr);
+				break;
+			case 0x2b:
+				display_generic_5_off_ic_gmask(ptr);
+				break;
+			default:
+				printk("unsupported opcode %x\n",
+					opcode);
+				break;
+		}
+	}
+}
