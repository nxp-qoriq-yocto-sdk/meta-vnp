+                (((t_FmPcdManip *)p_Manip->h_NextManip)->type == e_FM_PCD_MANIP_HDR))
+            p_Manip = p_Manip->h_NextManip;
+    } else {
+        p_Manip = *h_ManipIter;
+    }
